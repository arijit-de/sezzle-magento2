<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Checkout\Model\CompositeConfigProvider">
        <arguments>
            <argument name="configProviders" xsi:type="array">
                <item name="sezzle" xsi:type="object">Sezzle\Payment\Model\SezzleConfigProvider</item>
            </argument>
        </arguments>
    </type>
	<preference for="Sezzle\Payment\Model\Api\ConfigInterface" type="Sezzle\Payment\Model\Api\Config"/>
	<preference for="Sezzle\Payment\Model\Api\ProcessorInterface" type="Sezzle\Payment\Model\Api\Processor"/>
	<preference for="Sezzle\Payment\Model\Config\Container\SezzleApiConfigInterface" type="Sezzle\Payment\Model\Config\Container\SezzleApiIdentity"/>
</config>
